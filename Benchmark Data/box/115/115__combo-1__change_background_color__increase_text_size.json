{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "Box plot with precomputed five-number summaries.",
  "width": 500,
  "padding": 5,
  "config": {
    "axisBand": {
      "bandPosition": 1,
      "tickExtra": true,
      "tickOffset": 0
    },
    "background": "lightgray",
    "axis": {
      "labelFontSize": 14
    }
  },
  "signals": [
    {
      "name": "plotWidth",
      "value": 60
    },
    {
      "name": "height",
      "update": "(plotWidth + 10) * 3"
    }
  ],
  "data": [
    {
      "name": "summary",
      "values": [
        {
          "Education": "Bachelors",
          "min": 20000,
          "q1": 30000,
          "median": 45000,
          "q3": 60000,
          "max": 80000
        },
        {
          "Education": "Masters",
          "min": 25000,
          "q1": 40000,
          "median": 55000,
          "q3": 75000,
          "max": 100000
        },
        {
          "Education": "Doctorate",
          "min": 30000,
          "q1": 50000,
          "median": 70000,
          "q3": 90000,
          "max": 120000
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "layout",
      "type": "band",
      "range": "height",
      "domain": {
        "data": "summary",
        "field": "Education"
      },
      "padding": 0.1
    },
    {
      "name": "xscale",
      "type": "linear",
      "range": "width",
      "round": true,
      "domain": [
        20000,
        120000
      ],
      "zero": false,
      "nice": true
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "zindex": 1,
      "labelFontSize": 14
    },
    {
      "orient": "left",
      "scale": "layout",
      "tickCount": 5,
      "zindex": 1,
      "labelFontSize": 14
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "data": "summary",
          "name": "education",
          "groupby": "Education"
        }
      },
      "encode": {
        "enter": {
          "yc": {
            "scale": "layout",
            "field": "Education",
            "band": 0.5
          },
          "height": {
            "signal": "plotWidth"
          },
          "width": {
            "signal": "width"
          }
        }
      },
      "marks": [
        {
          "type": "rect",
          "from": {
            "data": "education"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "black"
              },
              "height": {
                "value": 1
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "min"
              },
              "x2": {
                "scale": "xscale",
                "field": "max"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "education"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "steelblue"
              },
              "cornerRadius": {
                "value": 4
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "q1"
              },
              "x2": {
                "scale": "xscale",
                "field": "q3"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "education"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "aliceblue"
              },
              "width": {
                "value": 2
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "median"
              }
            }
          },
          "clip": true
        }
      ]
    }
  ],
  "background": "#fff5e6"
}