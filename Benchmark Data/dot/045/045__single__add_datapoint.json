{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories as individual points along an axis.",
  "width": 300,
  "height": 200,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "Debt Ratio",
          "Value": 0.45
        },
        {
          "Category": "Monthly Income",
          "Value": 3000
        },
        {
          "Category": "Number of Dependents",
          "Value": 2
        },
        {
          "Category": "Revolving Utilization",
          "Value": 0.85
        },
        {
          "Category": "Age",
          "Value": 35
        },
        {
          "Category": "Credit Score",
          "Value": 700
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Category"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "shape": {
            "value": "circle"
          },
          "size": {
            "value": 100
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      }
    }
  ]
}