{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "Dot plot showing values for categories in the TalkingData AdTracking Fraud Detection dataset.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "category": "Device A",
          "value": 120
        },
        {
          "category": "Device B",
          "value": 85
        },
        {
          "category": "Device C",
          "value": 150
        },
        {
          "category": "Device D",
          "value": 95
        },
        {
          "category": "Device E",
          "value": 110
        },
        {
          "category": "Device F",
          "value": 130
        },
        {
          "category": "Device G",
          "value": 105
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "yscale",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "xscale",
      "orient": "bottom",
      "title": "Device Category",
      "titleFontSize": 16,
      "labelFontSize": 12
    },
    {
      "scale": "yscale",
      "orient": "left",
      "title": "Value",
      "titleFontSize": 16,
      "labelFontSize": 12
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "category"
          },
          "y": {
            "scale": "yscale",
            "field": "value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 0.5
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 12
    }
  }
}