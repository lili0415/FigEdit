{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A line chart showing bike trips over time by user type.",
  "width": 600,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "table",
      "values": [
        {
          "date": "2023-01",
          "trips": 1200,
          "user_type": "Subscriber"
        },
        {
          "date": "2023-01",
          "trips": 800,
          "user_type": "Customer"
        },
        {
          "date": "2023-02",
          "trips": 1500,
          "user_type": "Subscriber"
        },
        {
          "date": "2023-02",
          "trips": 950,
          "user_type": "Customer"
        },
        {
          "date": "2023-03",
          "trips": 1700,
          "user_type": "Subscriber"
        },
        {
          "date": "2023-03",
          "trips": 1100,
          "user_type": "Customer"
        },
        {
          "date": "2023-04",
          "trips": 1900,
          "user_type": "Subscriber"
        },
        {
          "date": "2023-04",
          "trips": 1200,
          "user_type": "Customer"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "range": "width",
      "padding": 0.7000000000000001,
      "domain": {
        "data": "table",
        "field": "date"
      }
    },
    {
      "name": "y",
      "type": "linear",
      "range": "height",
      "nice": true,
      "zero": true,
      "domain": {
        "data": "table",
        "field": "trips"
      }
    },
    {
      "name": "color",
      "type": "ordinal",
      "range": "category",
      "domain": {
        "data": "table",
        "field": "user_type"
      }
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "x",
      "title": "Date"
    },
    {
      "orient": "left",
      "scale": "y",
      "title": "Number of Trips"
    }
  ],
  "legends": [
    {
      "stroke": "color",
      "title": "User Type",
      "orient": "right"
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "name": "series",
          "data": "table",
          "groupby": "user_type"
        }
      },
      "marks": [
        {
          "type": "line",
          "from": {
            "data": "series"
          },
          "encode": {
            "enter": {
              "x": {
                "scale": "x",
                "field": "date"
              },
              "y": {
                "scale": "y",
                "field": "trips"
              },
              "stroke": {
                "scale": "color",
                "field": "user_type"
              },
              "strokeWidth": {
                "value": 2
              }
            }
          }
        }
      ]
    }
  ]
}