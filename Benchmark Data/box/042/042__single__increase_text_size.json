{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "Box plot with precomputed five-number summaries for UCI: Seeds dataset.",
  "width": 500,
  "padding": 5,
  "config": {
    "axisBand": {
      "bandPosition": 1,
      "tickExtra": true,
      "tickOffset": 0
    },
    "axis": {
      "labelFontSize": 12
    }
  },
  "signals": [
    {
      "name": "plotWidth",
      "value": 60
    },
    {
      "name": "height",
      "update": "(plotWidth + 10) * 3"
    }
  ],
  "data": [
    {
      "name": "summary",
      "values": [
        {
          "Group": "Kama",
          "min": 10.5,
          "q1": 12.0,
          "median": 13.5,
          "q3": 14.5,
          "max": 16.0
        },
        {
          "Group": "Rosa",
          "min": 9.8,
          "q1": 11.2,
          "median": 12.7,
          "q3": 14.0,
          "max": 15.5
        },
        {
          "Group": "Canadian",
          "min": 10.0,
          "q1": 11.5,
          "median": 13.0,
          "q3": 14.2,
          "max": 15.8
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "layout",
      "type": "band",
      "range": "height",
      "domain": {
        "data": "summary",
        "field": "Group"
      },
      "padding": 0.1
    },
    {
      "name": "xscale",
      "type": "linear",
      "range": "width",
      "round": true,
      "domain": [
        9.8,
        16.0
      ],
      "zero": false,
      "nice": true
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "zindex": 1,
      "encode": {
        "labels": {
          "update": {
            "fontSize": {
              "value": 14
            }
          }
        }
      },
      "labelFontSize": 12
    },
    {
      "orient": "left",
      "scale": "layout",
      "tickCount": 5,
      "zindex": 1,
      "encode": {
        "labels": {
          "update": {
            "fontSize": {
              "value": 14
            }
          }
        }
      },
      "labelFontSize": 12
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "data": "summary",
          "name": "groups",
          "groupby": "Group"
        }
      },
      "encode": {
        "enter": {
          "yc": {
            "scale": "layout",
            "field": "Group",
            "band": 0.5
          },
          "height": {
            "signal": "plotWidth"
          },
          "width": {
            "signal": "width"
          }
        }
      },
      "marks": [
        {
          "type": "rule",
          "from": {
            "data": "groups"
          },
          "encode": {
            "enter": {
              "stroke": {
                "value": "black"
              },
              "strokeWidth": {
                "value": 1
              },
              "y": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "x": {
                "scale": "xscale",
                "field": "min"
              },
              "x2": {
                "scale": "xscale",
                "field": "max"
              },
              "y": {
                "signal": "plotWidth / 2"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "groups"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "steelblue"
              },
              "cornerRadius": {
                "value": 4
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "q1"
              },
              "x2": {
                "scale": "xscale",
                "field": "q3"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "groups"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "aliceblue"
              },
              "width": {
                "value": 2
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "median"
              }
            }
          },
          "clip": true
        }
      ]
    }
  ]
}