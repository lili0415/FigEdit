{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories as individual points along an axis.",
  "width": 400,
  "height": 200,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "Kernels",
          "Value": 15.6
        },
        {
          "Category": "Seeds",
          "Value": 14.8
        },
        {
          "Category": "Grains",
          "Value": 16.2
        },
        {
          "Category": "Husks",
          "Value": 15.1
        },
        {
          "Category": "Shells",
          "Value": 14.5
        },
        {
          "Category": "Pulp",
          "Value": 15.9
        },
        {
          "Category": "Fibers",
          "Value": 15.3
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "range": "height",
      "nice": true,
      "zero": false
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Category"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 50
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "#4682b4"
          },
          "stroke": {
            "value": "#000"
          },
          "strokeWidth": {
            "value": 1
          }
        },
        "update": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          }
        }
      }
    }
  ]
}