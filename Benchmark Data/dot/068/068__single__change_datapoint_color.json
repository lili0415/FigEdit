{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories in the UCI: Yeast Dataset.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "CYT",
          "Value": 0.65
        },
        {
          "Category": "NUC",
          "Value": 0.87
        },
        {
          "Category": "MIT",
          "Value": 0.45
        },
        {
          "Category": "ME3",
          "Value": 0.72
        },
        {
          "Category": "EXC",
          "Value": 0.55
        },
        {
          "Category": "VAC",
          "Value": 0.38
        },
        {
          "Category": "POX",
          "Value": 0.49
        },
        {
          "Category": "ERL",
          "Value": 0.63
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Category"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "orange"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      }
    }
  ]
}