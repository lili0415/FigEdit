{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories in the Pima Indians Diabetes Database.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "Glucose Level",
          "Value": 85
        },
        {
          "Category": "Glucose Level",
          "Value": 120
        },
        {
          "Category": "Glucose Level",
          "Value": 140
        },
        {
          "Category": "BMI",
          "Value": 25.6
        },
        {
          "Category": "BMI",
          "Value": 30.5
        },
        {
          "Category": "BMI",
          "Value": 35.2
        },
        {
          "Category": "Blood Pressure",
          "Value": 72
        },
        {
          "Category": "Blood Pressure",
          "Value": 80
        },
        {
          "Category": "Blood Pressure",
          "Value": 90
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "yscale",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "xscale",
      "orient": "bottom",
      "title": "Category",
      "titleFontSize": 10,
      "labelFontSize": 8
    },
    {
      "scale": "yscale",
      "orient": "left",
      "title": "Value",
      "titleFontSize": 10,
      "labelFontSize": 8
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "Category"
          },
          "y": {
            "scale": "yscale",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 8
    }
  }
}