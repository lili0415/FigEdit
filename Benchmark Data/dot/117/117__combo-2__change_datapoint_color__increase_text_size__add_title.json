{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories in the Retailrocket dataset.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "table",
      "values": [
        {
          "Category": "Electronics",
          "Value": 120
        },
        {
          "Category": "Clothing",
          "Value": 85
        },
        {
          "Category": "Home & Kitchen",
          "Value": 95
        },
        {
          "Category": "Books",
          "Value": 60
        },
        {
          "Category": "Toys",
          "Value": 70
        },
        {
          "Category": "Beauty",
          "Value": 50
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "color",
      "type": "ordinal",
      "domain": [
        "Electronics",
        "Clothing",
        "Home & Kitchen",
        "Books",
        "Toys",
        "Beauty"
      ],
      "range": [
        "red",
        "steelblue",
        "steelblue",
        "steelblue",
        "steelblue",
        "steelblue"
      ]
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Category",
      "labelFontSize": 16
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value",
      "labelFontSize": 16
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "table"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "fill": {
            "field": "Category",
            "scale": "color"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      }
    }
  ],
  "title": {
    "text": "Edited Chart",
    "fontSize": 20
  },
  "config": {
    "axis": {
      "labelFontSize": 16
    }
  }
}