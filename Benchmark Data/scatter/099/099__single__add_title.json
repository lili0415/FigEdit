{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A scatter plot showing economic data from FRED.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "title": {
    "text": "Edited Chart"
  },
  "data": [
    {
      "name": "source",
      "values": [
        {
          "GDP": 21000,
          "UnemploymentRate": 3.5,
          "Region": "Northeast"
        },
        {
          "GDP": 18000,
          "UnemploymentRate": 4.2,
          "Region": "Midwest"
        },
        {
          "GDP": 25000,
          "UnemploymentRate": 3.0,
          "Region": "South"
        },
        {
          "GDP": 22000,
          "UnemploymentRate": 3.8,
          "Region": "West"
        },
        {
          "GDP": 19000,
          "UnemploymentRate": 4.5,
          "Region": "Northeast"
        },
        {
          "GDP": 23000,
          "UnemploymentRate": 3.2,
          "Region": "Midwest"
        },
        {
          "GDP": 26000,
          "UnemploymentRate": 2.8,
          "Region": "South"
        },
        {
          "GDP": 21000,
          "UnemploymentRate": 3.9,
          "Region": "West"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "domain": [
        18000.0,
        26000.0
      ],
      "range": "width",
      "nice": true,
      "zero": false
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "UnemploymentRate"
      },
      "range": "height",
      "nice": true,
      "zero": true
    },
    {
      "name": "color",
      "type": "ordinal",
      "domain": {
        "data": "source",
        "field": "Region"
      },
      "range": {
        "scheme": "category10"
      }
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "GDP (in billions)",
      "grid": true
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Unemployment Rate (%)",
      "grid": true
    }
  ],
  "legends": [
    {
      "fill": "color",
      "title": "Region",
      "orient": "right"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "GDP"
          },
          "y": {
            "scale": "y",
            "field": "UnemploymentRate"
          },
          "fill": {
            "scale": "color",
            "field": "Region"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          }
        }
      }
    }
  ]
}