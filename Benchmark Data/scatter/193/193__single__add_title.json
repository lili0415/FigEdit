{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A scatter plot showing car evaluation metrics.",
  "width": 300,
  "height": 300,
  "padding": 5,
  "title": {
    "text": "Edited Chart",
    "anchor": "start",
    "fontSize": 14
  },
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Buying_Price": 4.0,
          "Maintenance_Cost": 3.5,
          "Safety": "High"
        },
        {
          "Buying_Price": 2.5,
          "Maintenance_Cost": 4.0,
          "Safety": "Medium"
        },
        {
          "Buying_Price": 3.0,
          "Maintenance_Cost": 2.0,
          "Safety": "Low"
        },
        {
          "Buying_Price": 4.5,
          "Maintenance_Cost": 3.0,
          "Safety": "High"
        },
        {
          "Buying_Price": 3.5,
          "Maintenance_Cost": 3.5,
          "Safety": "Medium"
        },
        {
          "Buying_Price": 2.0,
          "Maintenance_Cost": 4.5,
          "Safety": "Low"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "linear",
      "nice": true,
      "domain": [
        2.0,
        4.5
      ],
      "range": "width",
      "zero": false
    },
    {
      "name": "y",
      "type": "linear",
      "nice": true,
      "domain": {
        "data": "source",
        "field": "Maintenance_Cost"
      },
      "range": "height"
    },
    {
      "name": "color",
      "type": "ordinal",
      "domain": {
        "data": "source",
        "field": "Safety"
      },
      "range": [
        "#1f77b4",
        "#ff7f0e",
        "#2ca02c"
      ]
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Buying Price"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Maintenance Cost"
    }
  ],
  "legends": [
    {
      "fill": "color",
      "title": "Safety",
      "orient": "right"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Buying_Price"
          },
          "y": {
            "scale": "y",
            "field": "Maintenance_Cost"
          },
          "fill": {
            "scale": "color",
            "field": "Safety"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          }
        }
      }
    }
  ]
}