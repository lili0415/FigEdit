{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing individual values for car evaluation categories.",
  "width": 400,
  "height": 200,
  "padding": 5,
  "autosize": "pad",
  "background": "#fff5e6",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "Buying",
          "Value": 3
        },
        {
          "Category": "Buying",
          "Value": 4
        },
        {
          "Category": "Maint",
          "Value": 2
        },
        {
          "Category": "Maint",
          "Value": 5
        },
        {
          "Category": "Doors",
          "Value": 4
        },
        {
          "Category": "Doors",
          "Value": 3
        },
        {
          "Category": "Persons",
          "Value": 5
        },
        {
          "Category": "Persons",
          "Value": 4
        },
        {
          "Category": "Lug_boot",
          "Value": 3
        },
        {
          "Category": "Lug_boot",
          "Value": 2
        },
        {
          "Category": "Safety",
          "Value": 5
        },
        {
          "Category": "Safety",
          "Value": 4
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "range": "height",
      "nice": true
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Category",
      "labelFontSize": 14
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value",
      "grid": true,
      "labelFontSize": 14
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 0.5
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 14
    }
  }
}