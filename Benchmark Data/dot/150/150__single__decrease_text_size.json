{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories in the UCI Yeast Dataset.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "CYT",
          "Value": 0.52
        },
        {
          "Category": "NUC",
          "Value": 0.61
        },
        {
          "Category": "MIT",
          "Value": 0.48
        },
        {
          "Category": "ME3",
          "Value": 0.45
        },
        {
          "Category": "EXC",
          "Value": 0.39
        },
        {
          "Category": "VAC",
          "Value": 0.42
        },
        {
          "Category": "POX",
          "Value": 0.37
        },
        {
          "Category": "ERL",
          "Value": 0.33
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "yscale",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "xscale",
      "orient": "bottom",
      "title": "Category",
      "titleFontSize": 10,
      "labelFontSize": 8
    },
    {
      "scale": "yscale",
      "orient": "left",
      "title": "Value",
      "titleFontSize": 10,
      "labelFontSize": 8
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "Category"
          },
          "y": {
            "scale": "yscale",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "#1f77b4"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 8
    }
  }
}