{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories from US Census ACS data.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "Age 18-24",
          "Value": 15.2
        },
        {
          "Category": "Age 25-34",
          "Value": 22.8
        },
        {
          "Category": "Age 35-44",
          "Value": 19.7
        },
        {
          "Category": "Age 45-54",
          "Value": 17.3
        },
        {
          "Category": "Age 55-64",
          "Value": 13.4
        },
        {
          "Category": "Age 65+",
          "Value": 11.6
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Age Group",
      "titleFontSize": 16,
      "labelFontSize": 12
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Percentage (%)",
      "titleFontSize": 16,
      "labelFontSize": 12
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 0.5
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 12
    }
  }
}