{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A line chart showing unemployment rate trends and labor force participation rate over time.",
  "width": 600,
  "height": 300,
  "padding": 5,
  "title": {
    "text": "Edited Chart",
    "fontSize": 18
  },
  "data": [
    {
      "name": "table",
      "values": [
        {
          "date": "2023-01",
          "unemployment_rate": 3.5,
          "labor_force_participation": 62.4
        },
        {
          "date": "2023-02",
          "unemployment_rate": 3.6,
          "labor_force_participation": 62.3
        },
        {
          "date": "2023-03",
          "unemployment_rate": 3.4,
          "labor_force_participation": 62.5
        },
        {
          "date": "2023-04",
          "unemployment_rate": 3.5,
          "labor_force_participation": 62.6
        },
        {
          "date": "2023-05",
          "unemployment_rate": 3.7,
          "labor_force_participation": 62.7
        },
        {
          "date": "2023-06",
          "unemployment_rate": 3.6,
          "labor_force_participation": 62.8
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "range": "width",
      "domain": {
        "data": "table",
        "field": "date"
      },
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "range": "height",
      "nice": true,
      "zero": true,
      "domain": {
        "data": "table",
        "fields": [
          "unemployment_rate",
          "labor_force_participation"
        ]
      }
    },
    {
      "name": "color",
      "type": "ordinal",
      "range": "category",
      "domain": [
        "Unemployment Rate",
        "Labor Force Participation"
      ]
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "x",
      "title": "Date",
      "labelFontSize": 16,
      "titleFontSize": 18,
      "tickFontSize": 10
    },
    {
      "orient": "left",
      "scale": "y",
      "title": "Percentage (%)",
      "labelFontSize": 16,
      "titleFontSize": 18,
      "tickFontSize": 10
    }
  ],
  "legends": [
    {
      "stroke": "color",
      "title": "Series",
      "orient": "right"
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "name": "series",
          "data": "table",
          "groupby": "series_key"
        }
      },
      "marks": [
        {
          "type": "line",
          "from": {
            "data": "series"
          },
          "encode": {
            "enter": {
              "x": {
                "scale": "x",
                "field": "date"
              },
              "y": {
                "scale": "y",
                "field": "value"
              },
              "stroke": {
                "scale": "color",
                "field": "series_key"
              },
              "strokeWidth": {
                "value": 2
              }
            },
            "update": {
              "strokeOpacity": {
                "value": 1
              }
            },
            "hover": {
              "strokeOpacity": {
                "value": 0.5
              }
            }
          }
        }
      ]
    }
  ],
  "transform": [
    {
      "type": "fold",
      "fields": [
        "unemployment_rate",
        "labor_force_participation"
      ],
      "as": [
        "series_key",
        "value"
      ]
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 16
    }
  }
}