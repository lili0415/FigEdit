{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories in FRED Economic Data.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "background": "#fff5e6",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "GDP (Trillions)",
          "Value": 21.7
        },
        {
          "Category": "Unemployment Rate (%)",
          "Value": 3.8
        },
        {
          "Category": "Inflation Rate (%)",
          "Value": 2.5
        },
        {
          "Category": "Federal Funds Rate (%)",
          "Value": 5.25
        },
        {
          "Category": "Consumer Spending (Trillions)",
          "Value": 14.9
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Economic Indicators"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Values"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 0.5
          }
        }
      }
    }
  ]
}