{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories as individual points along an axis.",
  "width": 400,
  "height": 200,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "Fresh Produce",
          "Value": 120
        },
        {
          "Category": "Dairy",
          "Value": 85
        },
        {
          "Category": "Bakery",
          "Value": 95
        },
        {
          "Category": "Frozen Foods",
          "Value": 110
        },
        {
          "Category": "Snacks",
          "Value": 75
        },
        {
          "Category": "Beverages",
          "Value": 90
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Category"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 50
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "orange"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      }
    }
  ]
}