{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for different job categories.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Job": "Management",
          "Conversion_Rate": 15.2
        },
        {
          "Job": "Technician",
          "Conversion_Rate": 12.8
        },
        {
          "Job": "Entrepreneur",
          "Conversion_Rate": 10.5
        },
        {
          "Job": "Blue-Collar",
          "Conversion_Rate": 8.3
        },
        {
          "Job": "Unemployed",
          "Conversion_Rate": 5.1
        },
        {
          "Job": "Student",
          "Conversion_Rate": 18.4
        },
        {
          "Job": "Retired",
          "Conversion_Rate": 9.7
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Job"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Conversion_Rate"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Job Category"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Conversion Rate (%)"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Job"
          },
          "y": {
            "scale": "y",
            "field": "Conversion_Rate"
          },
          "size": {
            "value": 100
          },
          "fill": {
            "value": "steelblue"
          }
        }
      }
    }
  ]
}