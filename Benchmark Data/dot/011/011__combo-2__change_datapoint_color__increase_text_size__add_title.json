{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing values for categories as individual points along an axis.",
  "width": 400,
  "height": 200,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "Fresh Produce",
          "Value": 120
        },
        {
          "Category": "Dairy",
          "Value": 85
        },
        {
          "Category": "Bakery",
          "Value": 95
        },
        {
          "Category": "Frozen Foods",
          "Value": 110
        },
        {
          "Category": "Snacks",
          "Value": 75
        },
        {
          "Category": "Beverages",
          "Value": 90
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "color",
      "type": "ordinal",
      "domain": [
        "Fresh Produce",
        "Dairy",
        "Bakery",
        "Frozen Foods",
        "Snacks",
        "Beverages"
      ],
      "range": [
        "steelblue",
        "orange",
        "steelblue",
        "steelblue",
        "steelblue",
        "steelblue"
      ]
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Category",
      "labelFontSize": 14
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value",
      "labelFontSize": 14
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 50
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "field": "Category",
            "scale": "color"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      }
    }
  ],
  "title": {
    "text": "Edited Chart",
    "fontSize": 18
  }
}