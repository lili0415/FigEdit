{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "Box plot with precomputed five-number summaries.",
  "width": 500,
  "padding": 5,
  "config": {
    "axisBand": {
      "bandPosition": 1,
      "tickExtra": true,
      "tickOffset": 0
    },
    "axis": {
      "labelFontSize": 12
    }
  },
  "signals": [
    {
      "name": "plotWidth",
      "value": 60
    },
    {
      "name": "height",
      "update": "(plotWidth + 10) * 3"
    }
  ],
  "data": [
    {
      "name": "summary",
      "values": [
        {
          "Region": "East Asia & Pacific",
          "min": 1000,
          "q1": 3000,
          "median": 5000,
          "q3": 7000,
          "max": 9000
        },
        {
          "Region": "Europe & Central Asia",
          "min": 2000,
          "q1": 4000,
          "median": 6000,
          "q3": 8000,
          "max": 10000
        },
        {
          "Region": "Latin America & Caribbean",
          "min": 1500,
          "q1": 3500,
          "median": 5500,
          "q3": 7500,
          "max": 9500
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "layout",
      "type": "band",
      "range": "height",
      "domain": {
        "data": "summary",
        "field": "Region"
      },
      "padding": 0.1
    },
    {
      "name": "xscale",
      "type": "linear",
      "range": "width",
      "round": true,
      "domain": [
        1000,
        10000
      ],
      "zero": false,
      "nice": true
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "zindex": 1,
      "encode": {
        "labels": {
          "update": {
            "fontSize": {
              "value": 14
            }
          }
        }
      },
      "labelFontSize": 12
    },
    {
      "orient": "left",
      "scale": "layout",
      "tickCount": 5,
      "zindex": 1,
      "encode": {
        "labels": {
          "update": {
            "fontSize": {
              "value": 14
            }
          }
        }
      },
      "labelFontSize": 12
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "data": "summary",
          "name": "regions",
          "groupby": "Region"
        }
      },
      "encode": {
        "enter": {
          "yc": {
            "scale": "layout",
            "field": "Region",
            "band": 0.5
          },
          "height": {
            "signal": "plotWidth"
          },
          "width": {
            "signal": "width"
          }
        }
      },
      "marks": [
        {
          "type": "rect",
          "from": {
            "data": "regions"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "black"
              },
              "height": {
                "value": 1
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "min"
              },
              "x2": {
                "scale": "xscale",
                "field": "max"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "regions"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "steelblue"
              },
              "cornerRadius": {
                "value": 4
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "q1"
              },
              "x2": {
                "scale": "xscale",
                "field": "q3"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "regions"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "aliceblue"
              },
              "width": {
                "value": 2
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "median"
              }
            }
          },
          "clip": true
        }
      ]
    }
  ]
}