{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A scatter plot showing relationships between Age and Annual Premium, categorized by Vehicle Damage status.",
  "width": 300,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Age": 25,
          "Annual_Premium": 30000,
          "Vehicle_Damage": "Yes"
        },
        {
          "Age": 30,
          "Annual_Premium": 40000,
          "Vehicle_Damage": "No"
        },
        {
          "Age": 35,
          "Annual_Premium": 50000,
          "Vehicle_Damage": "Yes"
        },
        {
          "Age": 40,
          "Annual_Premium": 60000,
          "Vehicle_Damage": "No"
        },
        {
          "Age": 45,
          "Annual_Premium": 70000,
          "Vehicle_Damage": "Yes"
        },
        {
          "Age": 50,
          "Annual_Premium": 80000,
          "Vehicle_Damage": "No"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": [
        25,
        30,
        35,
        40,
        45,
        50
      ],
      "range": "width"
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Annual_Premium"
      },
      "range": "height",
      "nice": true,
      "zero": true
    },
    {
      "name": "color",
      "type": "ordinal",
      "domain": {
        "data": "source",
        "field": "Vehicle_Damage"
      },
      "range": [
        "#1f77b4",
        "#ff7f0e"
      ]
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Age",
      "grid": true
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Annual Premium",
      "grid": true
    }
  ],
  "legends": [
    {
      "fill": "color",
      "title": "Vehicle Damage",
      "orient": "right"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Age"
          },
          "y": {
            "scale": "y",
            "field": "Annual_Premium"
          },
          "fill": {
            "scale": "color",
            "field": "Vehicle_Damage"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "opacity": {
            "value": 0.7
          }
        }
      }
    }
  ]
}