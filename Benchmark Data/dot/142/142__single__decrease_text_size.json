{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing individual points for categories.",
  "width": 400,
  "height": 200,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Education": "Bachelors",
          "HoursPerWeek": 40
        },
        {
          "Education": "Masters",
          "HoursPerWeek": 50
        },
        {
          "Education": "High School",
          "HoursPerWeek": 35
        },
        {
          "Education": "Doctorate",
          "HoursPerWeek": 60
        },
        {
          "Education": "Associates",
          "HoursPerWeek": 45
        },
        {
          "Education": "Some College",
          "HoursPerWeek": 38
        },
        {
          "Education": "High School",
          "HoursPerWeek": 42
        },
        {
          "Education": "Bachelors",
          "HoursPerWeek": 48
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Education"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "HoursPerWeek"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Education Level",
      "titleFontSize": 10,
      "labelFontSize": 8
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Hours Per Week",
      "titleFontSize": 10,
      "labelFontSize": 8
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Education"
          },
          "y": {
            "scale": "y",
            "field": "HoursPerWeek"
          },
          "size": {
            "value": 100
          },
          "fill": {
            "value": "steelblue"
          },
          "opacity": {
            "value": 0.7
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 8
    }
  }
}