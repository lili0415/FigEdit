  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": [
        2,
        4,
        14,
        22,
        26,
        27,
        28,
        30,
        35,
        38,
        54
      ],
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Fare"
      },
      "range": "height",
      "nice": true,
      "zero": true
    },
    {
      "name": "color",
      "type": "ordinal",
      "domain": {
        "data": "source",
        "field": "Survived"
      },
      "range": [
        "#e41a1c",
        "#377eb8"
      ]
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Age"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Fare"
    }
  ],
  "legends": [
    {
      "fill": "color",
      "title": "Survived",
      "orient": "right"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Age"
          },
          "y": {
            "scale": "y",
            "field": "Fare"
          },
          "fill": {
            "scale": "color",
            "field": "Survived"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "tooltip": {
            "signal": "{'Age': datum.Age, 'Fare': datum.Fare, 'Survived': datum.Survived}"
          }
        }
      }
    }
  ]
}