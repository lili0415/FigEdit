{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "width": 400,
  "height": 300,
  "padding": 5,
  "autosize": "pad",
  "title": {
    "text": "Edited Chart"
  },
  "data": [
    {
      "name": "source",
      "values": [
        {
          "category": "Bananas",
          "value": 120
        },
        {
          "category": "Apples",
          "value": 150
        },
        {
          "category": "Oranges",
          "value": 90
        },
        {
          "category": "Grapes",
          "value": 110
        },
        {
          "category": "Berries",
          "value": 130
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "yscale",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "title": "Category"
    },
    {
      "orient": "left",
      "scale": "yscale",
      "title": "Value"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "category"
          },
          "y": {
            "scale": "yscale",
            "field": "value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          }
        }
      }
    }
  ]
}