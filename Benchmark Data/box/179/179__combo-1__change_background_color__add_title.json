{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "Box plot with precomputed five-number summaries.",
  "width": 500,
  "padding": 5,
  "config": {
    "axisBand": {
      "bandPosition": 1,
      "tickExtra": true,
      "tickOffset": 0
    }
  },
  "signals": [
    {
      "name": "plotWidth",
      "value": 60
    },
    {
      "name": "height",
      "update": "(plotWidth + 10) * 3"
    }
  ],
  "data": [
    {
      "name": "summary",
      "values": [
        {
          "Workclass": "Private",
          "min": 15000,
          "q1": 25000,
          "median": 35000,
          "q3": 45000,
          "max": 60000
        },
        {
          "Workclass": "Self-emp",
          "min": 20000,
          "q1": 30000,
          "median": 40000,
          "q3": 50000,
          "max": 70000
        },
        {
          "Workclass": "Government",
          "min": 18000,
          "q1": 28000,
          "median": 38000,
          "q3": 48000,
          "max": 65000
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "layout",
      "type": "band",
      "range": "height",
      "domain": {
        "data": "summary",
        "field": "Workclass"
      },
      "padding": 0.1
    },
    {
      "name": "xscale",
      "type": "linear",
      "range": "width",
      "round": true,
      "domain": [
        15000,
        70000
      ],
      "zero": false,
      "nice": true
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "zindex": 1
    },
    {
      "orient": "left",
      "scale": "layout",
      "tickCount": 5,
      "zindex": 1
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "data": "summary",
          "name": "workclass",
          "groupby": "Workclass"
        }
      },
      "encode": {
        "enter": {
          "yc": {
            "scale": "layout",
            "field": "Workclass",
            "band": 0.5
          },
          "height": {
            "signal": "plotWidth"
          },
          "width": {
            "signal": "width"
          }
        }
      },
      "marks": [
        {
          "type": "rect",
          "from": {
            "data": "workclass"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "black"
              },
              "height": {
                "value": 1
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "min"
              },
              "x2": {
                "scale": "xscale",
                "field": "max"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "workclass"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "steelblue"
              },
              "cornerRadius": {
                "value": 4
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "q1"
              },
              "x2": {
                "scale": "xscale",
                "field": "q3"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "workclass"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "aliceblue"
              },
              "width": {
                "value": 2
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "median"
              }
            }
          },
          "clip": true
        }
      ]
    }
  ],
  "background": "#fff5e6",
  "title": {
    "text": "Edited Chart"
  }
}