{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "Dot plot showing values for categories as individual points along an axis.",
  "width": 400,
  "height": 200,
  "padding": 5,
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "Consumer Price Index",
          "Value": 260.5
        },
        {
          "Category": "Unemployment Rate",
          "Value": 3.8
        },
        {
          "Category": "GDP Growth Rate",
          "Value": 2.1
        },
        {
          "Category": "Federal Funds Rate",
          "Value": 5.25
        },
        {
          "Category": "Inflation Rate",
          "Value": 4.2
        },
        {
          "Category": "Personal Savings Rate",
          "Value": 8.3
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "range": "height",
      "nice": true,
      "zero": false
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Category",
      "labelAngle": -45,
      "labelFontSize": 16
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value",
      "labelFontSize": 16
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 16
    }
  }
}