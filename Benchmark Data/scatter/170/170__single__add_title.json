{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A scatter plot showing relationship between Age and Annual Premium, colored by Vehicle Damage status.",
  "width": 300,
  "height": 300,
  "padding": 5,
  "title": {
    "text": "Edited Chart"
  },
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Age": 25,
          "Annual_Premium": 30000,
          "Vehicle_Damage": "Yes"
        },
        {
          "Age": 45,
          "Annual_Premium": 40000,
          "Vehicle_Damage": "No"
        },
        {
          "Age": 35,
          "Annual_Premium": 35000,
          "Vehicle_Damage": "Yes"
        },
        {
          "Age": 50,
          "Annual_Premium": 45000,
          "Vehicle_Damage": "No"
        },
        {
          "Age": 30,
          "Annual_Premium": 32000,
          "Vehicle_Damage": "Yes"
        },
        {
          "Age": 40,
          "Annual_Premium": 38000,
          "Vehicle_Damage": "No"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": [
        25,
        30,
        35,
        40,
        45,
        50
      ],
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Annual_Premium"
      },
      "range": "height",
      "nice": true
    },
    {
      "name": "color",
      "type": "ordinal",
      "domain": {
        "data": "source",
        "field": "Vehicle_Damage"
      },
      "range": [
        "#1f77b4",
        "#ff7f0e"
      ]
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Age"
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Annual Premium"
    }
  ],
  "legends": [
    {
      "fill": "color",
      "title": "Vehicle Damage"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Age"
          },
          "y": {
            "scale": "y",
            "field": "Annual_Premium"
          },
          "fill": {
            "scale": "color",
            "field": "Vehicle_Damage"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          }
        }
      }
    }
  ]
}