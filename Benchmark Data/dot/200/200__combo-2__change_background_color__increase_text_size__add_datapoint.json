{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "width": 400,
  "height": 300,
  "padding": 5,
  "background": "#fff5e6",
  "data": [
    {
      "name": "table",
      "values": [
        {
          "Category": "Electronics",
          "Value": 120
        },
        {
          "Category": "Books",
          "Value": 80
        },
        {
          "Category": "Home Appliances",
          "Value": 150
        },
        {
          "Category": "Clothing",
          "Value": 100
        },
        {
          "Category": "Toys",
          "Value": 90
        },
        {
          "Category": "Furniture",
          "Value": 110
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "point",
      "domain": {
        "data": "table",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "yscale",
      "type": "linear",
      "domain": {
        "data": "table",
        "field": "Value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "title": "Category",
      "labelFontSize": 16
    },
    {
      "orient": "left",
      "scale": "yscale",
      "title": "Value",
      "labelFontSize": 16
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "table"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "Category"
          },
          "y": {
            "scale": "yscale",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 16
    }
  }
}