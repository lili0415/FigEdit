{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A bar chart showing category versus value.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "table",
      "values": [
        {
          "category": "Category A",
          "value": 45
        },
        {
          "category": "Category B",
          "value": 78
        },
        {
          "category": "Category C",
          "value": 62
        },
        {
          "category": "Category D",
          "value": 91
        },
        {
          "category": "Category E",
          "value": 53
        },
        {
          "category": "Category F",
          "value": 87
        },
        {
          "category": "Category G",
          "value": 34
        },
        {
          "category": "Category H",
          "value": 65
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "band",
      "domain": {
        "data": "table",
        "field": "category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "yscale",
      "type": "linear",
      "domain": {
        "data": "table",
        "field": "value"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "title": "Categories"
    },
    {
      "orient": "left",
      "scale": "yscale",
      "title": "Values"
    }
  ],
  "marks": [
    {
      "type": "rect",
      "from": {
        "data": "table"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "category"
          },
          "width": {
            "scale": "xscale",
            "band": 1
          },
          "y": {
            "scale": "yscale",
            "field": "value"
          },
          "y2": {
            "scale": "yscale",
            "value": 0
          },
          "fill": {
            "value": "steelblue"
          }
        },
        "update": {}
      }
    }
  ]
}