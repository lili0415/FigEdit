{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing economic indicators for different categories.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "background": "#fff5e6",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Category": "GDP Growth",
          "Value": 2.5
        },
        {
          "Category": "Unemployment Rate",
          "Value": 4.2
        },
        {
          "Category": "Inflation Rate",
          "Value": 1.8
        },
        {
          "Category": "Interest Rate",
          "Value": 0.5
        },
        {
          "Category": "Consumer Confidence",
          "Value": 98.4
        },
        {
          "Category": "Housing Starts",
          "Value": 1.3
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "band",
      "domain": {
        "data": "source",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.2
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Value"
      },
      "range": "height",
      "nice": true,
      "zero": true
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Economic Indicators",
      "labelAngle": -45
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Value"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Category"
          },
          "y": {
            "scale": "y",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 1
          }
        }
      }
    }
  ]
}