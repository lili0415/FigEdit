{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing the number of routes by airport.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "routes",
      "values": [
        {
          "Airport": "ATL",
          "Routes": 275
        },
        {
          "Airport": "LAX",
          "Routes": 245
        },
        {
          "Airport": "ORD",
          "Routes": 220
        },
        {
          "Airport": "DFW",
          "Routes": 210
        },
        {
          "Airport": "JFK",
          "Routes": 190
        },
        {
          "Airport": "DEN",
          "Routes": 185
        },
        {
          "Airport": "SFO",
          "Routes": 170
        },
        {
          "Airport": "SEA",
          "Routes": 160
        },
        {
          "Airport": "LAS",
          "Routes": 150
        },
        {
          "Airport": "MIA",
          "Routes": 140
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "routes",
        "field": "Airport"
      },
      "range": "width",
      "padding": 0.7000000000000001
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "routes",
        "field": "Routes"
      },
      "range": "height",
      "nice": true,
      "zero": true
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Airport",
      "labelAngle": -45
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Number of Routes"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "routes"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Airport"
          },
          "y": {
            "scale": "y",
            "field": "Routes"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "#1f77b4"
          },
          "stroke": {
            "value": "#000"
          }
        }
      }
    }
  ]
}