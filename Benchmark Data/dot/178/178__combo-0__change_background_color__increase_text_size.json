{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A dot plot showing trip durations for different taxi pickup locations.",
  "width": 400,
  "height": 300,
  "padding": 5,
  "autosize": "pad",
  "background": "#fff5e6",
  "data": [
    {
      "name": "source",
      "values": [
        {
          "Pickup_Location": "Manhattan",
          "Trip_Duration": 540
        },
        {
          "Pickup_Location": "Brooklyn",
          "Trip_Duration": 1200
        },
        {
          "Pickup_Location": "Queens",
          "Trip_Duration": 850
        },
        {
          "Pickup_Location": "Bronx",
          "Trip_Duration": 600
        },
        {
          "Pickup_Location": "Staten Island",
          "Trip_Duration": 1500
        },
        {
          "Pickup_Location": "Manhattan",
          "Trip_Duration": 300
        },
        {
          "Pickup_Location": "Brooklyn",
          "Trip_Duration": 950
        },
        {
          "Pickup_Location": "Queens",
          "Trip_Duration": 700
        },
        {
          "Pickup_Location": "Bronx",
          "Trip_Duration": 400
        },
        {
          "Pickup_Location": "Staten Island",
          "Trip_Duration": 1700
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "domain": {
        "data": "source",
        "field": "Pickup_Location"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "data": "source",
        "field": "Trip_Duration"
      },
      "nice": true,
      "range": "height"
    }
  ],
  "axes": [
    {
      "scale": "x",
      "orient": "bottom",
      "title": "Pickup Location",
      "labelFontSize": 16
    },
    {
      "scale": "y",
      "orient": "left",
      "title": "Trip Duration (seconds)",
      "labelFontSize": 16
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "source"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "x",
            "field": "Pickup_Location"
          },
          "y": {
            "scale": "y",
            "field": "Trip_Duration"
          },
          "size": {
            "value": 50
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "steelblue"
          },
          "stroke": {
            "value": "black"
          },
          "strokeWidth": {
            "value": 0.5
          }
        }
      }
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 16
    }
  }
}