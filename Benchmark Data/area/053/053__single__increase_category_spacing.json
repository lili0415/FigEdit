{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "An area chart showing the FAO Food Price Index over time.",
  "width": 600,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "table",
      "values": [
        {
          "year": 2010,
          "index": 161.5
        },
        {
          "year": 2011,
          "index": 229.9
        },
        {
          "year": 2012,
          "index": 213.6
        },
        {
          "year": 2013,
          "index": 209.8
        },
        {
          "year": 2014,
          "index": 201.8
        },
        {
          "year": 2015,
          "index": 164.0
        },
        {
          "year": 2016,
          "index": 161.5
        },
        {
          "year": 2017,
          "index": 174.6
        },
        {
          "year": 2018,
          "index": 168.4
        },
        {
          "year": 2019,
          "index": 171.5
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "point",
      "range": "width",
      "domain": [
        2010,
        2011,
        2012,
        2013,
        2014,
        2015,
        2016,
        2017,
        2018,
        2019
      ],
      "padding": 0.6
    },
    {
      "name": "yscale",
      "type": "linear",
      "range": "height",
      "nice": true,
      "domain": {
        "data": "table",
        "field": "index"
      }
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "title": "Year"
    },
    {
      "orient": "left",
      "scale": "yscale",
      "title": "FAO Food Price Index"
    }
  ],
  "marks": [
    {
      "type": "area",
      "from": {
        "data": "table"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "year"
          },
          "y": {
            "scale": "yscale",
            "field": "index"
          },
          "y2": {
            "scale": "yscale",
            "value": 0
          },
          "fill": {
            "value": "green"
          }
        },
        "update": {}
      }
    }
  ]
}