{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "Box plot with precomputed five-number summaries.",
  "width": 500,
  "padding": 5,
  "config": {
    "axisBand": {
      "bandPosition": 1,
      "tickExtra": true,
      "tickOffset": 0
    },
    "background": "lightgray"
  },
  "signals": [
    {
      "name": "plotWidth",
      "value": 60
    },
    {
      "name": "height",
      "update": "(plotWidth + 10) * 3"
    }
  ],
  "data": [
    {
      "name": "summary",
      "values": [
        {
          "Region": "North",
          "min": 10,
          "q1": 15,
          "median": 20,
          "q3": 25,
          "max": 30
        },
        {
          "Region": "South",
          "min": 12,
          "q1": 18,
          "median": 22,
          "q3": 28,
          "max": 35
        },
        {
          "Region": "East",
          "min": 8,
          "q1": 14,
          "median": 19,
          "q3": 23,
          "max": 29
        },
        {
          "Region": "West",
          "min": 11,
          "q1": 16,
          "median": 21,
          "q3": 26,
          "max": 32
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "layout",
      "type": "band",
      "range": "height",
      "domain": {
        "data": "summary",
        "field": "Region"
      },
      "padding": 0.1
    },
    {
      "name": "xscale",
      "type": "linear",
      "range": "width",
      "round": true,
      "domain": [
        8,
        35
      ],
      "zero": false,
      "nice": true
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "xscale",
      "zindex": 1
    },
    {
      "orient": "left",
      "scale": "layout",
      "tickCount": 5,
      "zindex": 1
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "data": "summary",
          "name": "regions",
          "groupby": "Region"
        }
      },
      "encode": {
        "enter": {
          "yc": {
            "scale": "layout",
            "field": "Region",
            "band": 0.5
          },
          "height": {
            "signal": "plotWidth"
          },
          "width": {
            "signal": "width"
          }
        }
      },
      "marks": [
        {
          "type": "rect",
          "from": {
            "data": "regions"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "black"
              },
              "height": {
                "value": 1
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "min"
              },
              "x2": {
                "scale": "xscale",
                "field": "max"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "regions"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "steelblue"
              },
              "cornerRadius": {
                "value": 4
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "q1"
              },
              "x2": {
                "scale": "xscale",
                "field": "q3"
              }
            }
          },
          "clip": true
        },
        {
          "type": "rect",
          "from": {
            "data": "regions"
          },
          "encode": {
            "enter": {
              "fill": {
                "value": "aliceblue"
              },
              "width": {
                "value": 2
              },
              "yc": {
                "signal": "plotWidth / 2"
              }
            },
            "update": {
              "yc": {
                "signal": "plotWidth / 2"
              },
              "height": {
                "signal": "plotWidth / 2"
              },
              "x": {
                "scale": "xscale",
                "field": "median"
              }
            }
          },
          "clip": true
        }
      ]
    }
  ],
  "background": "#fff5e6"
}