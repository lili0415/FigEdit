{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "Dot plot showing values for categories from Santander Value Prediction dataset.",
  "width": 300,
  "height": 200,
  "padding": 5,
  "autosize": "pad",
  "data": [
    {
      "name": "data",
      "values": [
        {
          "Category": "Feature A",
          "Value": 120.5
        },
        {
          "Category": "Feature B",
          "Value": 95.3
        },
        {
          "Category": "Feature C",
          "Value": 150.8
        },
        {
          "Category": "Feature D",
          "Value": 80.2
        },
        {
          "Category": "Feature E",
          "Value": 110.7
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "xscale",
      "type": "point",
      "domain": {
        "data": "data",
        "field": "Category"
      },
      "range": "width",
      "padding": 0.1
    },
    {
      "name": "yscale",
      "type": "linear",
      "domain": {
        "data": "data",
        "field": "Value"
      },
      "range": "height",
      "nice": true
    }
  ],
  "axes": [
    {
      "scale": "xscale",
      "orient": "bottom",
      "title": "Category"
    },
    {
      "scale": "yscale",
      "orient": "left",
      "title": "Value"
    }
  ],
  "marks": [
    {
      "type": "symbol",
      "from": {
        "data": "data"
      },
      "encode": {
        "enter": {
          "x": {
            "scale": "xscale",
            "field": "Category"
          },
          "y": {
            "scale": "yscale",
            "field": "Value"
          },
          "size": {
            "value": 100
          },
          "shape": {
            "value": "circle"
          },
          "fill": {
            "value": "#ff6347"
          }
        }
      }
    }
  ]
}