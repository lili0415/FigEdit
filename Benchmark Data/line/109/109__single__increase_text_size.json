{
  "$schema": "https://vega.github.io/schema/vega/v6.json",
  "description": "A line chart showing trends in BMI and cholesterol levels across age groups.",
  "width": 600,
  "height": 300,
  "padding": 5,
  "data": [
    {
      "name": "table",
      "values": [
        {
          "age_group": "20-29",
          "value": 25.1,
          "series": "BMI"
        },
        {
          "age_group": "30-39",
          "value": 27.3,
          "series": "BMI"
        },
        {
          "age_group": "40-49",
          "value": 29.2,
          "series": "BMI"
        },
        {
          "age_group": "50-59",
          "value": 30.5,
          "series": "BMI"
        },
        {
          "age_group": "60-69",
          "value": 28.9,
          "series": "BMI"
        },
        {
          "age_group": "20-29",
          "value": 190,
          "series": "Cholesterol"
        },
        {
          "age_group": "30-39",
          "value": 200,
          "series": "Cholesterol"
        },
        {
          "age_group": "40-49",
          "value": 210,
          "series": "Cholesterol"
        },
        {
          "age_group": "50-59",
          "value": 220,
          "series": "Cholesterol"
        },
        {
          "age_group": "60-69",
          "value": 215,
          "series": "Cholesterol"
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "point",
      "range": "width",
      "domain": {
        "data": "table",
        "field": "age_group"
      },
      "padding": 0.1
    },
    {
      "name": "y",
      "type": "linear",
      "range": "height",
      "nice": true,
      "zero": false,
      "domain": {
        "data": "table",
        "field": "value"
      }
    },
    {
      "name": "color",
      "type": "ordinal",
      "range": "category",
      "domain": {
        "data": "table",
        "field": "series"
      }
    }
  ],
  "axes": [
    {
      "orient": "bottom",
      "scale": "x",
      "title": "Age Group",
      "titleFontSize": 16,
      "labelFontSize": 12
    },
    {
      "orient": "left",
      "scale": "y",
      "title": "Value",
      "titleFontSize": 16,
      "labelFontSize": 12
    }
  ],
  "legends": [
    {
      "stroke": "color",
      "title": "Series",
      "orient": "right"
    }
  ],
  "marks": [
    {
      "type": "group",
      "from": {
        "facet": {
          "name": "series",
          "data": "table",
          "groupby": "series"
        }
      },
      "marks": [
        {
          "type": "line",
          "from": {
            "data": "series"
          },
          "encode": {
            "enter": {
              "x": {
                "scale": "x",
                "field": "age_group"
              },
              "y": {
                "scale": "y",
                "field": "value"
              },
              "stroke": {
                "scale": "color",
                "field": "series"
              },
              "strokeWidth": {
                "value": 2
              }
            }
          }
        }
      ]
    }
  ],
  "config": {
    "axis": {
      "labelFontSize": 12
    }
  }
}